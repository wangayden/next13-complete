  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "build:stats": "ANALYZE=true pnpm run build",
    "build:production": "NODE_ENV=production pnpm run i18n:extract && pnpm run i18n:compile && pnpm run clean && pnpm run build",
    "i18n:extract": "mv _.babelrc .babelrc && lingui extract --clean && mv .babelrc _.babelrc",
    "i18n:compile": "mv _.babelrc .babelrc && lingui compile && mv .babelrc _.babelrc",
    "check-types": "tsc --noEmit --pretty"
  },
